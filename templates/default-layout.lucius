@font-face {
    font-family:                        open-dyslexic;
    src:                                url('@{StaticR opendyslexic_ttf}'); 
}

body, p {
    font-family:                        open-dyslexic;
    background-color:                   white;
    color:                              #{lightTextLow};
}

h1, h2, h3, h4, h5, h6 {
    color:                              #{lightTextHigh};
}

a, a:visited, a:active, a:hover {
    color:                              #{blue};
}
